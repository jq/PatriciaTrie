#!/usr/bin/env python

import sys
import argparse
import urllib2
import urllib
import args
import json
import zlib

pp = args.args()

for line in sys.stdin:
    s = line.strip()

    if not bool(s):
        continue

    req = urllib2.Request(pp.host, urllib.urlencode({"s": s}))
    response = urllib2.urlopen(req)
    data = json.loads(zlib.decompress(response.read(), 16+zlib.MAX_WBITS))
    print data
