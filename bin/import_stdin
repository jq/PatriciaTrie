#!/usr/bin/env python

import sys
import argparse
import urllib2
import urllib
import args
import json

pp = args.args()

for line in sys.stdin:
    s = line.strip()

    if not bool(s):
        continue

    req = urllib2.Request(pp.url, urllib.urlencode({"s": s}))
    response = urllib2.urlopen(req)
    data = json.loads(response.read())
    print data
