#!/usr/bin/env python

import json
import args
import urllib
import urllib2
import pprint

pp = args.args()
res = []

url = "%s?%s" % (pp.url, urllib.urlencode({"p": " ".join(pp.strings)}))
req = urllib2.Request(url)

try:
    response = urllib2.urlopen(req)
    data = json.loads(response.read(), encoding="UTF-8")
    res.append(data)
    pprint.pprint(data)
except urllib2.HTTPError, he:
    print he
    print he.headers