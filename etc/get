#!/usr/bin/env python

import json
import args
import urllib
import urllib2

pp = args.args()
res = {}

url = "%s/?%s" % (pp.host, urllib.urlencode({"key": " ".join(pp.keys)}))
req = urllib2.Request(url)
response = urllib2.urlopen(req)
data = json.loads(response.read())
res.update(data)
    
print json.dumps(data, indent=4)