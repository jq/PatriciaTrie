#!/usr/bin/env python

import json
import pprint
import urllib
import urllib2
import args

pp = args.args()
res = {}

for k in pp.keys:
    req = urllib2.Request(pp.host, urllib.urlencode({"s": k}))
    response = urllib2.urlopen(req)
    data = json.loads(response.read())
    res.update(data)

pprint.pprint(res)
